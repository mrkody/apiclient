{
    "name": "shopexpress/apiclient",
    "description": "ShopExpress API Client",
    "keywords": [
        "ShopExpress",
        "apiclient"
    ],
    "homepage": "https://github.com/ShopExpress/apiclient",
    "license": "MIT",
    "require": {
        "php" : ">=5.6.0",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*"
    },
    "autoload": {
        "psr-4": {
            "ShopExpress\\ApiClient\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ShopExpress\\ApiClient\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2-dev"
        }
    }
}
